<Window x:Class="_20._101_Zibereva_V3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_20._101_Zibereva_V3"
        mc:Ignorable="d"
        Title="Discipline" MinHeight="450" MinWidth="908" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="781*"/>
            <ColumnDefinition Width="11*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="23*"/>
            <RowDefinition Height="326*"/>
            <RowDefinition Height="85*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Column="1" Margin="0,23,0,0" Grid.RowSpan="2">
            <DataGrid x:Name="LoadData" Height="100" FontFamily="Bahnschrift"/>
        </StackPanel>
        <StackPanel Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="371,138,0,0" Grid.RowSpan="2">
            <Button x:Name="btn_search" Content="Поиск" Width="90" Click="btn_search_Click" FontSize="14" FontFamily="Bahnschrift"/>
        </StackPanel>

    </Grid>
</Window>
